    </Page.Resources>
    <Grid Background="#FF89E9FF">
        <Grid Margin="10,0,0,0">
            <Grid.RowDefinitions>
                <RowDefinition Height="50*"></RowDefinition>
                <RowDefinition Height="40*"></RowDefinition>
                <RowDefinition Height="40*"></RowDefinition>
                <RowDefinition Height="500*"></RowDefinition>
            </Grid.RowDefinitions>
            <StackPanel Orientation="Vertical" Grid.Row="0">
                <Border BorderThickness="0,0,0,1" BorderBrush="Black" Height="45" >
                    <StackPanel Orientation="Horizontal" Height="30" Grid.Row="0">
                        <Image Source="ms-appx:///Image/LeavelRecords-icon.png" Margin="0" Width="23" VerticalAlignment="Center"></Image>
                        <TextBlock Text="Welcome to search your Leave records" FontSize="21" VerticalAlignment="Center" Foreground="Red"></TextBlock>
                    </StackPanel>
                </Border>
                <Border BorderThickness="0,10,0,0" BorderBrush="#FF0A9DE2" Margin="-1,0,0,0"></Border>
            </StackPanel>
            <StackPanel Orientation="Horizontal" Grid.Row="1" VerticalAlignment="Top">
                <StackPanel Orientation="Horizontal" Width="460">
                    <TextBlock Text="Date from:" FontSize="21" VerticalAlignment="Center"/>
                    <StackPanel Orientation="Horizontal">
                        <MyControlTextBox:TextboxImageUserControl MyWidth="200" MyImageSoruce="ms-appx:///Image/calendar-icon.png"></MyControlTextBox:TextboxImageUserControl>
                    </StackPanel>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="To:" FontSize="21" VerticalAlignment="Center"></TextBlock>
                    <StackPanel Orientation="Horizontal">
                        <MyControlTextBox:TextboxImageUserControl MyWidth="200" MyImageSoruce="ms-appx:///Image/calendar-icon.png"></MyControlTextBox:TextboxImageUserControl>
                    </StackPanel>
                </StackPanel>
            </StackPanel>
            <Border Grid.Row="2" BorderBrush="Black" BorderThickness="0,0,0,1">
                <StackPanel Orientation="Horizontal" Grid.Row="2">
                    <StackPanel HorizontalAlignment="Left" Width="460" Orientation="Horizontal" Grid.Row="2" VerticalAlignment="Top">
                        <TextBlock Text="Name/Alias:" VerticalAlignment="Center" FontSize="21"></TextBlock>
                        <TextBox HorizontalAlignment="Left" Style="{StaticResource TextBoxStyle1}" x:Name="searchword" BorderBrush="#FF0A9DE2" PlaceholderText="e.g:v-fapeng" Width="190" Height="30">
                            <i:Interaction.Behaviors>
                                <core:EventTriggerBehavior EventName="TextChanged">
                                    <core:InvokeCommandAction Command="{Binding SearchCommand}" CommandParameter="{Binding ElementName=searchword, Path=Text}"/>
                                </core:EventTriggerBehavior>
                            </i:Interaction.Behaviors>
                        </TextBox>

                        <Image Source="ms-appx:///Image/QueryLeavel-icon.png" Width="30" Height="30"></Image>
                    </StackPanel>

                    <StackPanel Grid.Row="2" Orientation="Horizontal">
                        <TextBlock Text="Leave Type:" VerticalAlignment="Center" FontSize="21"></TextBlock>
                        <ComboBox BorderBrush="#FF0A9DE2"  VerticalAlignment="Center" Name="LeaveTypeComboBox" 
                                  Margin="0,7,0,6" Width="143" Style="{StaticResource ComboBoxStyle1}"
                                  SelectedValue="{Binding Source={StaticResource leaveType}, Path=leaveType, Mode=TwoWay}">
                            <!--<i:Interaction.Behaviors>
                                <core:EventTriggerBehavior EventName="SelectionChanged">
                                    <core:InvokeCommandAction Command="{Binding LeaveTypeChangeCommand}" CommandParameter="{Binding ElementName=LeaveTypeComboBox, Path=SelectedValue}"/>
                                </core:EventTriggerBehavior>
                            </i:Interaction.Behaviors>-->
                            <ComboBoxItem Content="All Type"></ComboBoxItem>
                            <ComboBoxItem Content="Sick Leave"></ComboBoxItem>
                            <ComboBoxItem Content="Annual Leave"></ComboBoxItem>
                            <ComboBoxItem Content="Flexible Leave"></ComboBoxItem>
                        </ComboBox>
                    </StackPanel>


                </StackPanel>
            </Border>
            <StackPanel Orientation="Vertical" Grid.Row="3">
                <TextBlock Text="Output:" FontSize="20" HorizontalAlignment="Left"></TextBlock>
                <Border Width="1000" HorizontalAlignment="Left" BorderThickness="2" BorderBrush="#FF0A9DE2">
                    <StackPanel Orientation="Vertical">
                        <StackPanel Background="#FF0A9DE2" Orientation="Horizontal" BorderThickness="0">
                            <TextBlock Width="100" FontWeight="Black" HorizontalAlignment="Left" Text="Alias" Margin="10,0,0,0" ></TextBlock>
                            <TextBlock Width="100" FontWeight="Black" HorizontalAlignment="Left" Text="Name"></TextBlock>
                            <TextBlock Width="150"  FontWeight="Black" HorizontalAlignment="Left" Text="LeaveHour"></TextBlock>
                            <TextBlock Width="200"  FontWeight="Black" HorizontalAlignment="Left" Text="FromDate"></TextBlock>
                            <TextBlock Width="200"  FontWeight="Black" HorizontalAlignment="Left" Text="EndDate"></TextBlock>
                            <TextBlock Width="150"  FontWeight="Black" HorizontalAlignment="Left" Text="LeaveType"></TextBlock>
                            <TextBlock Width="100"  FontWeight="Black" HorizontalAlignment="Left" Text="IsApprovel"></TextBlock>

                        </StackPanel>
                        <ListView ContainerContentChanging="MyListView_ContainerContentChanging" Name="MyListView" HorizontalAlignment="Left" Width="1000" ItemsSource="{Binding LeaveRecords}" Style="{StaticResource ListViewStyle1}">
                            <ListView.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">

                                        <TextBlock Width="100" HorizontalAlignment="Left" Text="{Binding Alias}" ></TextBlock>
                                        <TextBlock Width="100"  Text="{Binding EnglishName}"></TextBlock>
                                        <TextBlock  Width="150"  Text="{Binding TotalTime}"></TextBlock>
                                        <TextBlock Width="200"  Text="{Binding StartTime}"></TextBlock>
                                        <TextBlock  Width="200"  Text="{Binding EndTime}"></TextBlock>
                                        <TextBlock Width="150"  Text="{Binding LeaveType}"></TextBlock>
                                        <TextBlock  Width="100"  Text="{Binding IsApproved}"></TextBlock>
                                    </StackPanel>
                                </DataTemplate>
                            </ListView.ItemTemplate>
                        </ListView>
                    </StackPanel>

                </Border>
            </StackPanel>

        </Grid>
    </Grid>
</Page>
